	<queryString>
		<![CDATA[SELECT
concat(Marque,' ',NoConteneur) as MarqueNoTc
,Matricule,
concat(Nom_Chauffeur,' ',Prenom_Chauffeur) as Chauffeur
FROM Dossier_Conteneur
inner join chauffeur
on chauffeur.idChauffeur=dossier_conteneur.idChauffeur
inner join camion
on camion.idCamion=dossier_conteneur.idCamion
where FRAUDE='LEGALE' && idDossier=$P{IdDossier};]]>
	</queryString>
